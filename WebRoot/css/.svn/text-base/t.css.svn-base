@charset "utf-8";
/* CSS Document */

body,*{ margin:0px; padding:0px;}
body{ font:Arial, Helvetica, sans-serif;
      font-size:12px;
	  
}
img{ border:none;}
.tan{ width:236px;


bottom:0;
display:none;
overflow:hidden;
padding:0;
position:fixed;
right:5px;






}
.tan_top{ width:236px; height:40px; background:url(../imgage/ta11.gif) no-repeat; color:#fff; font-weight:bold; text-indent:16px; line-height:30px; _padding-top:8px;_height:32px; }
.tan_top span{ position:relative; margin-left:118px;}
.tan_main{ width:236px; background:url(../imgage/ta2.gif) repeat-y;  padding:0px 20px; font-size:12px; font-family:"宋体";}
.tan_main ul{ list-style-type:none; width:198px;}
.tan_main ul span{ float:right;}
.tan_main ul li{ background:url(../imgage/left_messagebg2.jpg) no-repeat 0px 9px; line-height:180%; text-indent:6px;}
.tan_main li a:link,.tan_main li a:visited{color:#4e4141; text-decoration:none;}
.tan_main li a:hover{ color:#CC3300; text-decoration:underline;}
.tan_btm{ width:236px; height:17px; background:url(../imgage/ta3.gif) no-repeat;}