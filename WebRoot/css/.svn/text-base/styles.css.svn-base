
.validator_text_tips {
	margin-left:10px;
	background:#D0FFFF;
	font-size: 12px;
	font-weight:normal;
	padding:3px 5px;
	color:#000000;
	border:1px solid #008000;
	text-align:left;
}

.validator_fail_tips {
	font-size:12px;
	border:1px #FF0000 solid;
	background:#FFF0F0;
	color:#000000;
	padding:3px 5px 3px 25px;
	margin-left:10px;
	background-image:url(../images/syserr.png);
	background-repeat:no-repeat;
	background-position: 4px 2px;
	text-align:left;
	width:300px;
}

.validator_succ_tips {
	margin-left:10px;
	border:none;
	padding:3px 5px 3px 25px;
	background-image:url(../images/sysok.png);
	background-repeat:no-repeat;
	background-position: 4px 2px;
	text-align:left;
}

.validator_focus_border {
	border:1px solid #008000;
	background:#D0FFFF;
}

.validator_fail_border {
	border:1px solid #FF8686;
	background:#FFE8E8;
}

.calendar {
	background:url(../images/calendar.png) no-repeat right;
}

#ui-datepicker-div{
	font-size:13px;
	width:220px;
}

.ui-datepicker select.ui-datepicker-month {width: 30%;}
.ui-datepicker select.ui-datepicker-year { width: 42%;}

.prompt_container {
	margin-left:10px;
	background-color:#D0FFFF;
	font-size: 12px;
	font-weight:normal;
	padding:3px 5px;
	color:#000000;
	border:1px solid #008000;
	text-align:left;
	width:300px;
	position:absolute;
	z-index:10000;
	display:none;
}